{
  "manifest_version": 3,
  "name": "Web Page Summarizer",
  "version": "1.0",
  "description": "Summarize any web page and extract keywords.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}